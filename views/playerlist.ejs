<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('layouts/head.ejs') -%>
</head>

<body class="ma">


    <%- include('layouts/header.ejs') -%>

        <section>
            <% users.forEach(function (user) { %>
                <article>
                    <span class="pic"></span>
                    <article>
                        <h4>
                            <%= user.name %>
                        </h4>
                        <article>
                            <p>
                                <%= user.topGame %>
                            </p>
                            <p>
                                <%= user.time %>
                            </p>
                        </article>
                    </article>
                    <a href="#">Full profile</a>
                </article>
                <% }) %>
        </section>
        <%- include('layouts/footer.ejs') -%>
</body>

</html>